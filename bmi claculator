CLIPS> (defrule Bmi 
=>
(printout t "your height in kg?" crlf)
(bind ?x (read))
(printout t "your highet in cm?" crlf)
(bind ?y (read))
(assert (bmiDev (/ ?y ?x)))
( If (>   bmiDev 25)
Then( printout t "you have normal body mess" crlf)
Else ( printout t "you don't have normal body mess" crlf) ) )

